<!DOCTYPE html>
<html lang="tr">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <meta name="description" content="" />
    <meta name="author" content="" />
    <title>ProgrammersBlog | @ViewBag.Title</title>
    @await Html.PartialAsync("_LayoutCssPartial")
</head>
<body class="sb-nav-fixed">
    @await Html.PartialAsync("_LayoutNavBarPartial")
    <div id="layoutSidenav">
        @await Html.PartialAsync("_LayoutSideBarNavPartial")
        <div id="layoutSidenav_content">
            <main>
                <div class="container-fluid">
                    @RenderBody()
                </div>
            </main>
            @await Html.PartialAsync("_LayoutFooterPartial")
        </div>
    </div>
    @await Html.PartialAsync("_LayoutScriptsPartial")
    @await RenderSectionAsync("Scripts", false) 
</body>
</html>
